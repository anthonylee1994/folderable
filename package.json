{
  "name": "folderable",
  "displayName": "Folderable",
  "repository": "git@github.com:anthonylee1994/folderable.git",
  "description": "Make your Module.xxx becomes Module/index.xxx or Module/index.xxx becomes Module.xxx",
  "version": "0.1.2",
  "publisher": "TIGODOnlineLimited",
  "engines": {
    "vscode": "^1.65.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:folderable.folderize"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "folderable.folderize",
        "title": "Folderize / Unfolderize"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "folderable.folderize",
          "group": "MyGroup@1",
          "when": "resourceScheme == file"
        }
      ],
      "commandPalette": [
        {
          "command": "folderable.folderize",
          "when": "false"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "publish": "vsce publish",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.0",
    "@types/node": "14.x",
    "@types/vscode": "^1.65.0",
    "@typescript-eslint/eslint-plugin": "^5.12.1",
    "@typescript-eslint/parser": "^5.12.1",
    "@vscode/test-electron": "^2.1.2",
    "eslint": "^8.9.0",
    "glob": "^7.2.0",
    "mocha": "^9.2.1",
    "ts-loader": "^9.2.6",
    "typescript": "^4.5.5",
    "webpack": "^5.69.1",
    "webpack-cli": "^4.9.2"
  }
}
